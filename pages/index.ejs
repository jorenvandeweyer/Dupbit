                        <a style="margin: 0px" href="https://discord.gg/yt9hyPd" align="right">
                            <img src="https://discordapp.com/api/guilds/310865351099285505/embed.png">
                        </a>
                    </div>
                </div>
				<div class="card-body">
					<p>Welcome to our new page.</p>
					<p>New projects coming soon.</p>
				</div>
			</div>
		</div>
        <%- include ./components/footer %>
